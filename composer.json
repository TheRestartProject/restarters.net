{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2|~8.0",
        "addwiki/mediawiki-api": "~0.7.0",
        "addwiki/mediawiki-api-base": "~2.0",
        "barryvdh/laravel-translation-manager": "^0.6.2",
        "bkwld/croppa": "^4.11",
        "caseyamcl/guzzle_retry_middleware": "^2.6",
        "cweagans/composer-patches": "^1.7",
        "darkaonline/l5-swagger": "6.*",
        "doctrine/dbal": "^2.6",
        "egulias/email-validator": "2.1.25",
        "fideloper/proxy": "^4.4",
        "filp/whoops": "^2.14",
        "fruitcake/laravel-cors": "^0.11.0",
        "guzzlehttp/guzzle": "^6.3",
        "hieu-le/wordpress-xmlrpc-client": "^2.6",
        "intervention/image": "^2.7",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^6.20",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.2",
        "league/csv": "^9.7",
        "mariuzzo/laravel-js-localization": "^1.6",
        "mcamara/laravel-localization": "^1.3",
        "msurguy/honeypot": "^1.0",
        "owen-it/laravel-auditing": "^12.0",
        "phpxmlrpc/polyfill-xmlrpc": "^1.0@RC",
        "sentry/sentry-laravel": "^2.9",
        "soundasleep/html2text": "^1.1",
        "spatie/calendar-links": "^1.6",
        "spinen/laravel-discourse-sso": "^2.0",
        "symfony/http-foundation": "^4.2.12",
        "twbs/bootstrap": "4.1.0",
        "wouternl/laravel-drip": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "laravel/dusk": "^6.19",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^3.0",
        "php-coveralls/php-coveralls": "^2.4",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/dom-crawler": "^5.3",
        "fakerphp/faker": "*",
        "facade/ignition": "^1.16.4"
    },
    "autoload": {
        "files": [
            "app/Helpers/Fixometer.php",
            "app/Helpers/FixometerFile.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "patches-file": "patches/composer.patches.json"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "cweagans/composer-patches": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
