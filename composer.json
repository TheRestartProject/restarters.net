{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "addwiki/mediawiki-api": "~0.7.0",
        "addwiki/mediawiki-api-base": "~2.0",
        "barryvdh/laravel-cors": "^0.11.0",
        "barryvdh/laravel-translation-manager": "^0.5.1",
        "caseyamcl/guzzle_retry_middleware": "^2.6",
        "cweagans/composer-patches": "^1.7",
        "doctrine/dbal": "^2.7",
        "egulias/email-validator": "2.1.25",
        "fideloper/proxy": "^4.0",
        "filp/whoops": "^2.9",
        "fzaninotto/faker": "1.9.2",
        "guzzlehttp/guzzle": "^6.3",
        "caseyamcl/guzzle_retry_middleware": "^2.6",
        "hieu-le/wordpress-xmlrpc-client": "~2.0",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "5.8.*",
        "laravelista/lumen-vendor-publish": "^2.1",
        "league/csv": "^9.1",
        "mariuzzo/laravel-js-localization": "^1.6",
        "mcamara/laravel-localization": "^1.3",
        "msurguy/honeypot": "^1.0",
        "owen-it/laravel-auditing": "^9.0",
        "sentry/sentry-laravel": "^2.7",
        "soundasleep/html2text": "^1.1",
        "spatie/calendar-links": "^1.2",
        "spinen/laravel-discourse-sso": "^2.0",
        "symfony/http-foundation": "^4.2.12",
        "twbs/bootstrap": "4.1.0",
        "wouternl/laravel-drip": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.0",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^3.0",
        "php-coveralls/php-coveralls": "^2.4",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/dom-crawler": "^5.2"
    },
    "autoload": {
        "files": [
            "app/Helpers/Fixometer.php",
            "app/Helpers/FixometerFile.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "patches-file": "patches/composer.patches.json"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
