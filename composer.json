{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "addwiki/mediawiki-api": "^3.1.0",
        "addwiki/mediawiki-api-base": "^3.1.0",
        "barryvdh/laravel-translation-manager": "^0.6.2",
        "bkwld/croppa": "^7.0",
        "caseyamcl/guzzle_retry_middleware": "^2.6",
        "cweagans/composer-patches": "^1.7",
        "darkaonline/l5-swagger": "^8.5",
        "doctrine/dbal": "^3.5",
        "egulias/email-validator": "^3.0.0",
        "filp/whoops": "^2.14",
        "geocoder-php/mapbox-provider": "^1.4",
        "guzzlehttp/guzzle": "^7.8",
        "hieu-le/wordpress-xmlrpc-client": "~2.0",
        "intervention/image-laravel": "^1.5.6",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^12.10",
        "laravel/tinker": "^2.10.1",
        "laravel/ui": "^4.6",
        "league/csv": "^9.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "mariuzzo/laravel-js-localization": "^2.0",
        "mcamara/laravel-localization": "^2.3.0",
        "msurguy/honeypot": "^1.4",
        "owen-it/laravel-auditing": "^14.0",
        "rennokki/laravel-eloquent-query-cache": "^3.6",
        "sentry/sentry-laravel": "^4.13",
        "soundasleep/html2text": "^1.1",
        "spatie/calendar-links": "^1.6",
        "spatie/laravel-validation-rules": "^3.4",
        "spatie/laravel-html": "^3.12.0",
        "spinen/laravel-discourse-sso": "^2.9",
        "symfony/http-client": "^7.2",
        "symfony/http-foundation": "^7.2",
        "symfony/mailgun-mailer": "^7.2",
        "toin0u/geocoder-laravel": "^5.0",
        "twbs/bootstrap": "4.1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "fakerphp/faker": "^1.23",
        "laravel/dusk": "^8.2",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "osteel/openapi-httpfoundation-testing": "^0.11",
        "phpunit/phpunit": "^11.5.3",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/dom-crawler": "^7.2",
        "laravel/pail": "^1.2.2"
    },
    "autoload": {
        "files": [
            "app/Helpers/Fixometer.php",
            "app/Helpers/FixometerFile.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "patches-file": "patches/composer.patches.json"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
