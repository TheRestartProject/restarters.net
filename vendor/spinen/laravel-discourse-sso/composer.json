{
    "name": "spinen/laravel-discourse-sso",
    "description": "Integrate Discourse SSO into Laravel.",
    "keywords": [
        "discourse",
        "laravel",
        "library",
        "single sign on",
        "spinen",
        "sso"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        },
        {
            "name": "Stephen Finney",
            "email": "stephen.finney@spinen.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "cviebrock/discourse-php": "^0.9.3",
        "guzzlehttp/guzzle": "^6.4|^7.0.1|^7.2",
        "illuminate/auth": "~5.5|~6|~7|~8|~9",
        "illuminate/routing": "~5.5|~6|~7|~8|~9",
        "illuminate/support": "~5.5|~6|~7|~8|~9"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.4|^9.0",
        "psy/psysh": "^0.10",
        "symfony/thanks": "^1.1",
        "symfony/var-dumper": "~3.0|^4.2|^6"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\Discourse\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\Discourse\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\Discourse\\SsoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
